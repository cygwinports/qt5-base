The gnu-libiconv test is solely for platforms where the inbuf parameter to
iconv(3) is const char **; we have GNU libiconv but use the POSIX char **.

--- origsrc/qtbase-opensource-src-5.3.2/config.tests/unix/iconv/iconv.pro	2014-09-11 05:48:07.000000000 -0500
+++ src/qtbase-opensource-src-5.3.2/config.tests/unix/iconv/iconv.pro	2014-10-24 12:10:50.588706500 -0500
@@ -1,3 +1,3 @@
 SOURCES = iconv.cpp
 CONFIG -= qt dylib
-mac|mingw|qnx:LIBS += -liconv
+mac|cygwin-g++*|mingw|qnx:LIBS += -liconv
--- origsrc/qtbase-opensource-src-5.3.2/src/corelib/codecs/qiconvcodec.cpp	2014-09-11 05:48:06.000000000 -0500
+++ src/qtbase-opensource-src-5.3.2/src/corelib/codecs/qiconvcodec.cpp	2014-10-24 12:11:32.038970000 -0500
@@ -472,7 +472,7 @@ iconv_t QIconvCodec::createIconv_t(const
         init();
 
     iconv_t cd = (iconv_t) -1;
-#if defined(__GLIBC__) || defined(GNU_LIBICONV) || defined(Q_OS_QNX)
+#if defined(__GLIBC__) || defined(GNU_LIBICONV) || defined(Q_OS_QNX) || defined(Q_OS_CYGWIN)
 #if defined(Q_OS_QNX)
     // on QNX the default locale is UTF-8, and an empty string will cause iconv_open to fail
     static const char empty_codeset[] = "UTF-8";
